    <script>
    const api_url ="./api/v1";
    async function getapi(url) {
	  const response = await fetch(url);
	  var data = await response.json();
	  if (response) {
      // console.log(data);
	  }
    show(data);
    }
    getapi(api_url);

    hideloader();
function hideloader() {
	document.getElementById('loading').style.display = 'none';
}

function openInNewTab(href) {
  Object.assign(document.createElement('a'), {
    target: '_blank',
    href: href,
  }).click();
}

function show(data) {
	for (let r of data) {
        if(Date.now() >= Date.parse(r.mtime)) {
            openInNewTab(r.murl);
			    fetch(`./api/v1/delete?mid=${r.mid}`)
			   .then(response => response.json())
			   .then(data => {
			  	if(data.status=="200 OK") {
					if (data.message=="Deleted") {
						console.log("Deleted");
					}
				}
			   }
				).catch(function(err) {
				 console.log(err);}
			   );
        }
	}
}
setTimeout(function(){
   window.location.reload(1);
}, 30*1000);
</script> 
