{
  "name":"lamp",
  "description": "attend meetings while sleeping, working or at the party.",
  "version":"1.2",
  "manifest_version":2,
  "permissions": [
    "activeTab"
  ],

  "content_scripts":[
    {
    "matches":["<all_urls>"],
    "js":["content.js"]
    }
  ],
  "browser_action":{
    "default_popup":"lamp.html",
    "default_title":"lamp"
  }
}
